{
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-idiomatic-order",
    "stylelint-config-css-modules"
  ],
  "plugins": [
    "stylelint-scss"
  ],
  "ignoreFiles": [
    "**/*.css"
  ],
  "rules": {
    "no-descending-specificity": null,
    "value-keyword-case": null,
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true
  }
}
